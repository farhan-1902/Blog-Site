<%- include('partials/header') -%> 
<h1>Home</h1>
<p><%= homeContent %></p>

<% posts.forEach(function(post) { %>
    <h2><%= post.title %> </h2>
    <p style="display: inline;"><%= post.content.substring(0, 100) + '...'%></p>
    <a href="/posts/<%= post._id %>">Read More</a>
<% }); %>
<%- include('partials/footer') -%> 